{
  "name": "wdf",
  "version": "0.0.1",
  "description": "Web data frame",
  "main": "index.js",
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "devtest": "webpack-dev-server --config webpack.mocha.js",
    "test": "mocha",
    "build": "webpack && webpack --config webpack.uglified.js && mocha && webpack --config webpack.mocha.js",
    "zip": "cd dist && zip wdf.zip wdf*",
    "copy_public": "docco ./docs/*md -o dist && cp ./node_modules/mocha/mocha.css dist/ ",
    "docs": "docco ./wdf/*js ./docs/*md -t docs/docco.jst -c docs/docco.css -o dist",
    "all": "echo clean build zip copy_public docs|xargs -n 1 npm run",
    "cover": "mocha --require blanket -R html-cov > dist/coverage.html",
    "coveralls": "mocha --require blanket --reporter mocha-lcov-reporter | coveralls",
    "codecov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec  && codecov <./coverage/coverage.json",
    "travis": "echo clean build codecov|xargs -n 1 npm run",
    "gh": "gh-pages -d dist/",
    "dev": "webpack-dev-server --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walnutgeek/wdf.git"
  },
  "keywords": [
    "dataframe",
    "pandas",
    "data",
    "dataset",
    "R",
    "SAS",
    "python"
  ],
  "author": "wg@walnutgeek.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/walnutgeek/wdf/issues"
  },
  "homepage": "https://github.com/walnutgeek/wdf#readme",
  "devDependencies": {
    "blanket": "^1.2.1",
    "codecov": "^1.0.1",
    "coveralls": "^2.11.4",
    "docco": "^0.7.0",
    "ejs": "^2.3.4",
    "gh-pages": "^0.5.0",
    "istanbul": "^0.4.0",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "markdown": "^0.5.0",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "mocha-loader": "^0.7.1",
    "rimraf": "^2.4.3",
    "uglify-loader": "^1.2.0",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  }
}
